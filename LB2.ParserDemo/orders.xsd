		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="order" type="tns:Order"
						minOccurs="0" maxOccurs="unbounded" />
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
 </xsd:schema>